body {
    overflow-y: scroll;
}

.modal .modal-body {
    max-height: 420px;
    overflow-y: auto;
}

.control-sidebar-menu .menu-icon {
    width: 20px !important;
    height: 20px !important;
}

table th {
    text-align: center !important;
    vertical-align: middle !important;
}

table td {
    text-align: center;
    vertical-align: middle !important;
}

.shell {
    white-space: pre;
    font-family: Courier, monospace;
    width: 100%;
    height: 415px;
    background-color: black;
    color: #ffffff;
}

.select-checkbox {
    cursor: pointer;
}
